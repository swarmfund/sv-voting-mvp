  "dependencies": {
    "elm": "^0.18.0",
    "ethereumjs-testrpc": "^4.1.3",
    "ethereumjs-util": "^5.1.2",
    "sodium": "^2.0.1",
    "truffle": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "elm-live": "^2.7.5",
    "npm-run-all": "^4.1.1",
    "web3-typescript-typings": "^0.6.2"
  },
  "scripts": {
    "----- SMART CONTRACT COMMANDS": "",
    "solc": "cd contract && truffle compile",
    "_sc_testrpc_deploy": "run-p _testrpc _sc_deploy",
    "_testrpc": "testrpc",
    "_sc_deploy": "cd contract && truffle deploy",
    "----- WEB / ELM COMMANDS": "",
    "web": "yarn _live",
    "build-web": "",
    "_open_browser": "open http://localhost:8000/web/src/SecureVote/SPAs/SwarmMVP/Main.elm",
    "_reactor": "elm-reactor",
    "_live": "elm-live ./web/src/SecureVote/SPAs/SwarmMVP/Main.elm --output=./web/sv-swarm.js --dir=./web --open --debug",
    "----- FULL SYSTEM COMMANDS": "",
    "dry-run": "run-p _sc_testrpc_deploy"
  }
}
